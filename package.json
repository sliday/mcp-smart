{
  "name": "mcp-smart",
  "version": "1.1.0",
  "description": "MCP server providing multi-advisor AI consultations via OpenRouter API with advanced caching, rate limiting, and security features",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-smart": "dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "openrouter", 
    "llm",
    "ai",
    "advisor",
    "deepseek",
    "gemini",
    "openai",
    "multi-advisor",
    "caching",
    "rate-limiting"
  ],
  "author": "Stas Kulesh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sliday/mcp-smart.git"
  },
  "homepage": "https://github.com/sliday/mcp-smart#readme",
  "bugs": {
    "url": "https://github.com/sliday/mcp-smart/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0"
  }
}